    "vue-tsc": "^1.8.27",
    "zx": "^7.2.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "yarn lint"
    ],
    "*.{js,jsx,vue,md,json}": [
      "prettier --write --ignore-unknown"
    ],
    "*.{ts,tsx}": [
      "prettier --parser=typescript --write --ignore-unknown"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "yarn type"
    }
  },
  "main": "index.js",
  "web-types": "./web-types.json"

    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.20.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.33",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "^0.3.0",

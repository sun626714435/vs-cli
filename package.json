    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@types/json-stringify-safe": "^5.0.3",
    "@types/lodash": "^4.14.202",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.4",
    "@types/nprogress": "^0.2.3",
    "@types/numeral": "^2.0.5",
    "@types/qs": "^6.9.11",
